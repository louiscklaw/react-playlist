{
  "name": "react-beautiful-dnd-functional-component-tryout",
  "homepage": "https://aboutme.louislabs.com",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@atlaskit/theme": "^11.0.2",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "react": "^16.13.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "styled-components": "^5.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:dev": "react-scripts test --maxWorkers=1 --notify --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "prettier_file": "prettier --write \"./src/**/*.{js,jsx}\"",
    "clear-all": "rm -rf build node_modules",
    "re-build": "rm -rf build node_modules && yarn install && yarn build",
    "gitUpdate": "yarn prettier_file && git add . && git commit -m'update,' && git pull && git push",
    "dev": "yarn --dev && set HTTPS=true&&yarn start",
    "http": "serve -s build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "prettier": "^2.6.2"
  }
}
