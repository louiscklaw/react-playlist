{
  "name": "react-framer-motion-tryout",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "eslint-plugin-prettier": "^4.0.0",
    "framer-motion": "^4.1.17",
    "prettier": "^2.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:dev": "react-scripts test --maxWorkers=1 --notify --coverage --watchAll=false",
    "prettier_file": "prettier --write \"./src/**/*.{js,jsx}\"",
    "clear-all": "rm -rf build node_modules",
    "re-build": "rm -rf build node_modules && yarn install && yarn build",
    "eject": "react-scripts eject",
    "gitUpdate": "yarn prettier_file && powershell scripts/gitUpdate.ps1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "eqeqeq": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
