{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "printWidth": 80,
  "jsxBracketSameLine": true,
  "arrowParens": "always",
  "overrides": [
    {
      "files": ["**/*.html"],
      "options": {
        "printWidth": 320
      }
    },
    {
      "files": ["**/langs/**/*.js"],
      "options": {
        "printWidth": 320
      }
    }
  ]
}
