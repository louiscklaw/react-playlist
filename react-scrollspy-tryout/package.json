{
  "name": "react-scrollspy-tryout",
  "version": "0.1.0",
  "homepage": "https://purii.github.io/react-use-scrollspy",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^14.1.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.24",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-scripts": "5.0.1",
    "react-use-scrollspy": "^3.0.2",
    "typescript": "^4.6.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "test_coverage": "yarn test -- --coverage",
    "screenshot": "yarn test",
    "test:dev": "react-scripts test --maxWorkers=1 --notify --coverage --watchAll=false",
    "prettier_file": "prettier --write \"./src/**/*.{js,jsx}\"",
    "clear": "rm -rf build node_modules",
    "re-build": "rm -rf build node_modules && yarn install && yarn build",
    "gitUpdate": "git pull && git add . && git commit -m\"update react-scrollspy-tryout,\" && git push",
    "dev": "yarn --dev && npx kill-port 3000 && set HTTPS=true&&yarn start",
    "http": "serve -s build",
    "lint": "eslint -c ./.eslintrc.js src --ignore-pattern src/tests --ignore-pattern *.test.js --max-warnings 5 --fix",
    "lint_init": "eslint --init"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^3.2.3"
  }
}
