{
  "name": "metric-dashboard-tryout",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.6.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test_coverage": "yarn test -- --coverage",
    "screenshot": "yarn test",
    "test:dev": "react-scripts test --maxWorkers=1 --notify --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "prettier_file": "prettier --write \"./src/**/*.{js,jsx}\"",
    "trim": "rm -rf build node_modules",
    "re-build": "rm -rf build node_modules && yarn install && yarn build",
    "gitUpdate": "git add . && git commit -m\"update metric-dashboard-tryout,\" && git pull && git push",
    "dev": "yarn --dev && npx kill-port 3000 && set HTTPS=true&&yarn start",
    "lint": "eslint -c ./.eslintrc.js src --ignore-pattern src/tests --ignore-pattern *.test.js --max-warnings 5 --fix",
    "http": "serve -s build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "prettier": "latest",
    "puppeteer": "latest"
  }
}
