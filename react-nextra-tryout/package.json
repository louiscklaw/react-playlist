  "dependencies": {
    "next": "^10.0.0",
    "nextra": "^0.3.1",
    "nextra-theme-docs": "^0.3.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "prettier": "^2.0.5"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
